<!DOCTYPE html>
<html lang="en">
<head>
    <title>{{title}}</title>
    {{> meta}}
    {{> head}}
    {{> courseSchema}}
</head>
<body class="scroll-assist" style="margin-top: -70px;">
{{> navbar}}
<div class="main-container">
    {{> header}}
    {{> overview}}
    {{> coursehighlights}}
    {{> freeAccessBanner}}
	{{> coursecontents}}
    {{> team team=mentors}}
	<section>
		<section class="tabbed-enroll-section">
			<div class="container">
				<div class="row v-align-children">
					<div class="col-md-4 col-sm-6">
						<div class="pricing-table pt-1 text-center emphasis">
							<h5 class="uppercase">
								Regular Batch
							</h5>
							<div><span class="price">â‚¹8,000</span></div>
							<p class="lead">(10 lectures)</p>
							<a class="btn btn-white btn-lg" href="#register">Enroll Now</a>
							<p>
								<a href="/contactus">Contact Us</a>
								<br>Call(Toll-free): 1800-2744-504
							</p>
						</div>
					</div>
				</div>

			</div>
		</section>
	</section>
	{{> courseschedule}}
	<section>
		<a id="register"></a>
		<div class="text-center">
		<h3>REGISTER NOW</h3></div>
		<div class="container">
			<div class="course-tabs">
				<ul class="nav nav-tabs" role="tablist">
					<li role="presentation" class="wide-tab active" id="tab-dwarka"><a href="#dwarka" aria-controls="home" role="tab" data-toggle="tab" aria-expanded="true">Dwarka</a></li>
					<!-- <li role="presentation" class="wide&#45;tab" id="tab&#45;dehradun"><a href="#dehradun" aria&#45;controls="profile" role="tab" data&#45;toggle="tab" aria&#45;expanded="false">Dehradun</a></li> -->
				</ul>
				<div class="tab-content">
					<div role="tabpanel" class="tab-pane active" id="dwarka">
						<div class="widget-container" {{#if frame-id-dwarka}}data-widget-id='{{frame-id-dwarka}}'{{/if}}>
								<iframe class="payment-widget" {{#if frame-id-dwarka}}id="{{frame-id-dwarka}}"{{/if}} frameborder="0" src="{{{registerLinkDwarka}}}"></iframe>
							</div>
					</div>
					<div role="tabpanel" class="tab-pane" id="dehradun">
						<div class="widget-container" {{#if frame-id-dehradun}}data-widget-id='{{frame-id-dehradun}}'{{/if}}>
								<iframe class="payment-widget" {{#if frame-id-dehradun}}id="{{frame-id-dehradun}}"{{/if}} frameborder="0" src="{{{registerLinkDehradun}}}"></iframe>
							</div>
					</div>
				</div>
			</div>
		</div>
	</section>
    {{> coursefaq}}
    {{> map}}
    {{> footer}}
</div>
{{> scripts}}
</body>
</html>
