<!DOCTYPE html>
<html lang="en">
<head>
    <title>{{title}}</title>
    {{> meta}}
    {{> head}}
    {{> courseSchema}}
</head>
<body class="scroll-assist">
{{> navbar}}
<div class="main-container">
    {{> header}}
    {{> overview}}
    {{> coursehighlights}}
    {{> freeAccessBanner}}

    <a id="contents" class="in-page-link"></a>
    <section>
        <div class="container">
          <h2 class="uppercase mb16 text-center color-primary">Course Contents</h2>
          <div class="course-card-container">
              <div class="content-row">
                {{#each courseContents}}
                  <div class="course-card col-xs-12 col-sm-6 col-md-4 col-lg-4">
                    <h3 class="topic">{{ this.title }}</h3>
                    <hr>
                    <ul class="points-list">
                      {{#each this.points}}
                        <li>{{ this }}</li>
                      {{/each}}
                    </ul>
                  </div>
                {{/each}}
              </div>
          </div>
        </div>
      </section>



    {{> team team=mentors}}
	<section style="padding-top: 0; margin-top: -5rem;">
		<div class="container">
			<div class="row v-align-children">
				<div class="col-md-4 col-sm-6">
					<div class="pricing-table pt-1 text-center emphasis">
						<h5 class="uppercase">
							Regular Batch
						</h5>
						<div><span class="price">₹12,000</span></div>
						<!-- <span class="price">₹9,000</span> -->
						<p class="lead">(10-12 lectures)</p>
						<a class="btn btn-white btn-lg" href="#register">Enroll Now</a>
						<p>
							<a href="/contactus">Contact Us</a>
							<br>Call(Toll-free): 1800-2744-504
						</p>
					</div>
				</div>
			</div>

		</div>
	</section>
	  {{> courseschedule}}
	  <a id="register"></a>
	  <section id="register">
      <div class="text-center">
        <h3>REGISTER NOW</h3></div>
        <div class="container">
          <div class="course-tabs">
            <ul class="nav nav-tabs" role="tablist">
              <li role="presentation" class="wide-tab active" id="tab-pitampura"><a href="#pitampura" aria-controls="profile" role="tab" data-toggle="tab" aria-expanded="true">Pitampura</a></li>
              <li role="presentation" class="wide-tab" id="tab-noida"><a href="#noida" aria-controls="profile" role="tab" data-toggle="tab" aria-expanded="true">Noida (Early bird discount)</a></li>
            </ul>
            <div class="tab-content">
              <div role="tabpanel" class="tab-pane active" id="pitampura">
                <div class="container widget-container" data-widget-id="239"><iframe class="payment-widget" frameborder="0" src="https://students.codingblocks.com/payment/iframe?item_id=198"></iframe></div>
              </div>
              <div role="tabpanel" class="tab-pane" id="noida">
                <div class="container widget-container" data-widget-id="238"><iframe class="payment-widget" frameborder="0" src="https://students.codingblocks.com/payment/iframe?item_id=197"></iframe></div>
              </div>
            </div>
          </div>
      </div>
    </section>
    {{> coursefaq}}
    {{> map}}
    {{> footer}}
</div>
{{> scripts}}
</body>
</html>
