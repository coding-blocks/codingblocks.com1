{{> scripts}}
<script>
    $(document).ready(function () {
        var hash = window.location.hash;
        console.log('WORKING', hash);
        if (hash === '#complete' || hash === '#ad') {
            $('#tab-basic').toggleClass('active');
            $('#tab-complete').toggleClass('active');
            $('#content-basic').toggleClass('active');
            $('#content-complete').toggleClass('active');
        }
    })
</script>
