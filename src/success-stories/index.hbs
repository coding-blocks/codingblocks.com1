<!DOCTYPE html>
<html lang="en">
<head>
    <title>{{title}}</title>
    {{> meta}}
    {{> head}}
    <style>
        h4 {
            margin: 0;
        }   

        .flippy{
            max-width: 360px;
            margin-left: 1%;
            margin-right: 1%;
        }

        .front{
            width: calc(100% - 30px) !important;
        }

        .ssCard {
            background: white;
            border-radius: 20px;
            padding-top: 85px;
            padding-bottom: 20px;
            margin-bottom: 40px;
            margin-left: 10px;
            margin-right: 10px;
            height: 300px;
            width: 95%;
        }

        .ssImage {
            position: absolute;
            top: 0;
            width: 100%;
            z-index: 100;
        }

        .ssContainer {
            position: relative;
            padding-top: 75px;
        }

        .ssLogo {
            text-align: center;
        }

        .ssData {
            text-align: center;
            padding-top: 20px;
            cursor: pointer;
        }

        .ssStory {
            height: 100px;
            white-space: wrap;
            overflow: auto;
            text-overflow: ellipsis;
        }

        .courses {
            margin-bottom: 5px;
        }

        .ellipsis {
            padding-top: 10px;
            padding-left: 15px;
            padding-right: 15px;
            padding-bottom: 10px;  
            height: 150px;
            line-height: 20px;
            overflow: hidden;
            text-overflow: ellipsis;
        }
        

        .round-img {
            border-radius: 50%;
            border: 5px solid white;
            width: 150px;
            height: 150px;
            object-fit: cover;
            object-position: top;  
            {{!-- margin-left: -30px; --}}
        }

        .title {
            margin-top: 10px;
        }

        .highlight {
            background: url("../assets/images/success-stories/bg.jpg") no-repeat 0 0 fixed;
            background-size: cover;
            -webkit-background-size: cover;
            -moz-background-size: cover;
            -o-background-size: cover;
        }

        .highlightOverlay {
            background-color: #230c28;
            opacity: 1;
        }

        .highlight h2 {
            color: #fff;
            margin-top: 40px;
            font-size: 52px;
            font-weight: 500;
            text-shadow: 0px 0px 13px rgba(256, 256, 256, 0.7);
            transition: all 300ms linear;
        }

        .highlight h2:hover {
            text-shadow: 0px 0px 20px rgba(256, 256, 256, 0.9);
            transition: all 300ms linear
        }

        .flip-card {
            width: 26%;
            margin-left: 4%;
            margin-right: 3%;
            padding: 15px 15px !important;
            height: 100%;
        }

        .title {
            margin-top: 10px;
        }

        @media (max-width: 215px) {
            .flip-card {
                width: 96%;
                margin-left: 1%;
                margin-right: 1%;
            }
        }

        @media (min-width: 216px) and (max-width: 460px) {
            .flip-card {
                width: 90%;
                margin-left: 4%;
                margin-right: 4%;
            }
        }

        @media (min-width: 461px) and (max-width: 767px) {
            .flip-card {
                width: 45%;
                margin-left: 3%;
                margin-right: 0%;
            }
        }

        @media (min-width: 768px) and (max-width: 1024px) {
            .flip-card {
                width: 45%;
                margin-left: 3%;
                margin-right: 0%;
            }
        }

        .hover-light {
            -webkit-box-shadow: 0px 0px 15px 4px rgba(256, 256, 256, 0.3);
            -moz-box-shadow: 0px 0px 15px 4px rgba(256, 256, 256, 0.3);
            box-shadow: 0px 0px 15px 4px rgba(256, 256, 256, 0.3);
            transition: all 300ms linear
        }

        .hover-light:hover {
            -webkit-box-shadow: 0px 0px 35px 4px rgba(256, 256, 256, 0.5);
            -moz-box-shadow: 0px 0px 35px 4px rgba(256, 256, 256, 0.5);
            box-shadow: 0px 0px 35px 4px rgba(256, 256, 256, 0.5);
            transition: all 300ms linear
        }
    </style>
</head>
