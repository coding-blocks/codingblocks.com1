{{> scripts}}
<script>
	const Once = function (fn) {
		let hasRun = false,
			result = (void 0)
		;

		return function () {
			if (hasRun) return result

			hasRun = true

			result = fn.apply (this, arguments)

			return result
		}
	}

    $('.storyButton').click(function () {
        let parent = '#' + $(this).closest("div").prop("id");
        $(parent + ' .modal').modal("toggle");
    });

	$(window).load (function (event) {
		let images = $('.deferred-img-container img')

		images.each (function () {
			let image = $(this)
			image.attr ('src', image.data ('src'))
		})
	})

	$('.trigger-img-load').waypoint ({
		handler: Once (function (direction) {
			let images = $('.lazy-img-container img')

			images.each (function () {
				let image = $(this)
				image.attr ('src', image.data ('src'))
			})
		})
	})

    $.ajax ({
        url: "https://app.codingblocks.com/public_stories",
        type: "GET",
        success: function (stories) {
            var container = $('.team-2')
            for (i in stories) {

                let internship = stories[i].internships
                let placement = stories[i].placements
                let company = null

                if (placement[0]) {
                    company = placement[0].company.logoUrl
                    if (!company) {
                        company = 'http://logo.clearbit.com/' + placement[0].company.companyUrl
                    }
                } else {
                    company = internship[0].company.logoUrl
                    if (!company) {
                        company = 'http://logo.clearbit.com/' + internship[0].company.companyUrl
                    }
                }

                let pic = stories[i].photo
                if (! pic) {
                    pic = "../assets/images/default-avatar.jpg"
                }


              container.append(
                `
                <div class="flippy col-4 col-md-4 col-sm-6 col-xs-12">
                    <div class="front">    
                        <div class="ssContainer">    
                            <div class="ssImage">
                                <img class="round-img" src="${pic}" >
                            </div>
                            <div id="story${stories[i].id}" class="ssCard">
                                <div class="title mb0 text-center">
                                    <h4 class="uppercase mb0 bold">${stories[i].fullName}</h4>
                                    <h5 class="courses">${stories[i].coursesAttended}<br></h5>
                                </div>
                                <div class="ssLogo">
                                    <img width="64px" src="${company}" >
                                </div>
                                <div class="bold ssData">View Testimonial > </div>
                            </div>
                        </div>
                    </div>

                    <div class="back">    
                        <div class="ssContainer">
                            <div class="ssImage">
                                <img class="round-img" src="${pic}" >
                            </div>
                            <div id="story${stories[i].id}" class="ssCard">
                                <div class="title mb0 text-center">
									<h4 class="uppercase mb0 bold">${stories[i].fullName}</h4>
                                    <div class="ellipsis">${stories[i].stories}</div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>  
                
                `
              )
            }
            $('.flippy').flip({
                axis: 'y',
                trigger: 'click'
            })
        }
    })
</script>
