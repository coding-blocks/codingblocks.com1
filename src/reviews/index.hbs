            margin-bottom: 20px;
        }



        .hover-light-more {
            -webkit-box-shadow: 0px 0px 15px 5px rgba(0, 0, 0, 0.3);
            -moz-box-shadow: 0px 0px 15px 5px rgba(0, 0, 0, 0.3);
            box-shadow: 0px 0px 15px 5px rgba(0, 0, 0, 0.3);
            transition: all 300ms linear
        }

        .hover-light-more:hover {
            -webkit-box-shadow: 0px 0px 15px 5px rgba(0, 0, 0, 0.5);
            -moz-box-shadow: 0px 0px 15px 5px rgba(0, 0, 0, 0.5);
            box-shadow: 0px 0px 15px 5px rgba(0, 0, 0, 0.5);
            transition: all 300ms linear
        }

        .highlight {
            background: url("//v.fastcdn.co/u/e4110b44/16489001-0-programming-wallpape.jpg") fixed;
            background-repeat: no-repeat;
            background-position: top left;
            background-size: cover;
            -webkit-background-size: cover;
            -moz-background-size: cover;
            -o-background-size: cover;
        }

        .highlightOverlay {
            background-color: #2b2c33;
            opacity: 0.80722891566265;
        }

        .scale-anm {
            transform: scale(1);
        }

        .btn-shallow {
            background: #fff;
        }

        .eventlogo {
            height: 170px !important;
        }

        .event-list {
            list-style: none;
            margin: 0px;
            padding: 0px;
        }

        .event-list > li {
            display: none;
            background-color: #f8f8f8;
            box-shadow: 0px 0px 2px 1px rgba(0, 0, 0, 0.2);
            padding: 10px !important;
            height: auto !important;
            margin: 0px 0px 20px;
            transition: 300ms;
        }

        .event-list > li:nth-child(even) {
            background-color: #ffffff;
        }

        .event-list > li:hover {
            box-shadow: 0px 0px 6px 2px rgba(0, 0, 0, 0.2);
            transition: 300ms;
        }

        .event-list > li > img {
            width: 100%;
        }

        .event-list > li > .info > ul,
        .event-list > li > .info > ul > li,
        .event-list > li > .info > ul > li > a {
            display: block;
            width: 100%;
            color: rgb(30, 30, 30);
            text-decoration: none;
        }

        .event-list > li > .info > ul > li:hover {
            color: rgb(30, 30, 30);
            background-color: rgb(200, 200, 200);
        }

        .moreimgholder {
            width: 22%;
            display: inline-block;
            background: none !important;
            float: left;
        }

        .moreimgholder img {
            filter: none !important;
            opacity: 1 !important;
            height: auto;
        }



        .event-list > li > .reviewBox > .desc {
            font-size: 13pt;
            font-weight: 400;
            float: left !important;
        }

        .event-list > li > .reviewBox > .title {
            font-size: 14pt;
            font-weight: 600;
            float: left !important;
            margin-bottom: 5px;
        }

        q::before, q::after {
            font-size: 30px;
            font-weight: 600;
        }

        .flip-card .col-sm-8 {
            padding-top: 10px;
        }


    </style>
</head>
