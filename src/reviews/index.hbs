        $('#competitive-list li:lt(' + x + ')').css('display', 'inline-block');
        $('.loadMore').click(function () {
            let parent = '#' + $(this).closest("ul").prop("id");
            let size_li = $(parent + ' li').size();
            let x = $(parent + ' li:visible').size();
            x = (x + 3 <= size_li) ? x + 3 : size_li;
            $(parent + ' li:lt(' + x + ')').css('display', 'inline-block');
            $(parent + ' .showLess').css('display', 'inline-block');
            if (x === size_li)
                $(this).css('display', 'none');
        });
        $('.showLess').click(function () {
            let parent = '#' + $(this).closest("ul").prop("id");
            let size_li = $(parent + ' li').size();
            let x = $(parent + ' li:visible').size();
            if (x === size_li)
                $(parent + ' .loadMore').css('display', 'inline-block');
            x = (x - 3 < 2) ? 2 : x - 3;
            if (x === 2)
                $(this).css('display', 'none');
            $(parent + ' li').not(':lt(' + x + ')').css('display', 'none');
        });
    });
