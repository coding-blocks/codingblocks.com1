            $(this).toggleClass('btn-filled');
            $(this).removeClass('btn-shallow');
            selectedClass = $(this).attr("data-rel");
            console.log(selectedClass);
            $("#row1").fadeTo(100, 0.1);
            $("#row1 div").not("." + selectedClass).fadeOut().removeClass('scale-anm');
            setTimeout(function () {
                $("." + selectedClass).fadeIn().addClass('scale-anm');
                $("#row1").fadeTo(300, 1);
            }, 300);
        });
    });
</script>
</body>
</html>
